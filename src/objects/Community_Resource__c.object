    <fields>
        <fullName>Description__c</fullName>
        <description>Represents a description of the Resource that is presented in the Community.</description>
        <externalId>false</externalId>
        <inlineHelpText>Represents a description of the Resource that is presented in the Community.</inlineHelpText>
        <label>Description</label>
        <length>15000</length>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Html</type>
        <visibleLines>10</visibleLines>
    </fields>
    <fields>
        <fullName>Help_Link__c</fullName>
        <defaultValue>false</defaultValue>
        <description>if checked, link will be the Help link</description>
        <externalId>false</externalId>
        <label>Help Link</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Link__c</fullName>
        <description>Used to store a hyperlink for a resource that is either a website or a file hosted elsewhere that can be accessed with a direct link.</description>
        <externalId>false</externalId>
        <label>Link</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>Primary_Tag__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Define the primary tag used to categorize and organize this resource in the Resource Library list view.  Additional tags may also be associated separately.</description>
        <externalId>false</externalId>
        <label>Primary Tag</label>
        <lookupFilter>
            <active>false</active>
            <filterItems>
                <field>Community_Tag__c.Community_Theme__r.Theme_Type__c</field>
                <operation>equals</operation>
                <value>Resource</value>
            </filterItems>
            <infoMessage>You can only select Tags where the Theme Type = Resource.</infoMessage>
            <isOptional>true</isOptional>
        </lookupFilter>
        <referenceTo>Community_Tag__c</referenceTo>
        <relationshipLabel>Resources Tagged w Primary</relationshipLabel>
        <relationshipName>Resource_Tagged_w_Primary</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Quick_Link__c</fullName>
        <defaultValue>false</defaultValue>
        <description>if checked, link will be displayed in the &quot;Links&quot; menu</description>
        <externalId>false</externalId>
        <inlineHelpText>f checked, link will be displayed in the &quot;Links&quot; menu</inlineHelpText>
        <label>Quick Link</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Resource_Name__c</fullName>
        <description>Identifies the name of the Resource that will be displayed in the Community.</description>
        <externalId>false</externalId>
        <inlineHelpText>Identifies the name of the Resource that will be displayed in the Community.</inlineHelpText>
        <label>Resource Name</label>
        <length>150</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Resource_SF_ID__c</fullName>
        <description>Represents the 18 digit Salesforce Unique ID of the Resource record.</description>
        <externalId>false</externalId>
        <formula>CASESAFEID(Id)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Represents the 18 digit Salesforce Unique ID of the Resource record.</inlineHelpText>
        <label>Resource SF ID</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Sort__c</fullName>
        <description>To use to define sort order of Resources when viewing records on the CommunityResourceList page</description>
        <externalId>false</externalId>
        <label>Sort</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <description>The status of a resource will determine whether it is visible in the Community.</description>
        <externalId>false</externalId>
        <label>Status</label>
        <picklist>
            <picklistValues>
                <fullName>Active</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Inactive</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <label>Community Resource</label>
