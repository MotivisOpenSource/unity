    <label>Community Event</label>
    <listViews>
        <fullName>All_Events</fullName>
        <columns>Name__c</columns>
        <columns>NAME</columns>
        <columns>Public__c</columns>
        <columns>Start__c</columns>
        <columns>End__c</columns>
        <columns>Teaser__c</columns>
        <columns>CREATEDBY_USER</columns>
        <filterScope>Everything</filterScope>
        <label>All Events</label>
        <language>en_US</language>
    </listViews>
    <listViews>
        <fullName>Past_Events</fullName>
        <columns>Name__c</columns>
        <columns>NAME</columns>
        <columns>Start__c</columns>
        <columns>End__c</columns>
        <columns>Teaser__c</columns>
        <columns>CREATED_DATE</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Start__c</field>
            <operation>lessThan</operation>
            <value>TODAY</value>
        </filters>
        <label>Past Events</label>
        <language>en_US</language>
    </listViews>
    <listViews>
        <fullName>Todays_Events</fullName>
        <columns>Name__c</columns>
        <columns>NAME</columns>
        <columns>Start__c</columns>
        <columns>End__c</columns>
        <columns>Teaser__c</columns>
        <columns>CREATED_DATE</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Start__c</field>
            <operation>equals</operation>
            <value>TODAY</value>
        </filters>
        <label>Today&apos;s Events</label>
        <language>en_US</language>
    </listViews>
    <listViews>
        <fullName>Upcoming_Events</fullName>
        <columns>Name__c</columns>
        <columns>NAME</columns>
        <columns>Start__c</columns>
        <columns>End__c</columns>
        <columns>Teaser__c</columns>
        <columns>CREATED_DATE</columns>
        <columns>LAST_UPDATE</columns>
        <columns>CREATEDBY_USER</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Start__c</field>
            <operation>greaterOrEqual</operation>
            <value>TODAY</value>
        </filters>
        <label>Upcoming Events</label>
        <language>en_US</language>
    </listViews>
