    <fields>
        <fullName>Building__c</fullName>
        <description>Used to store the building name where the Event is taking place, if any.</description>
        <externalId>false</externalId>
        <label>Building</label>
        <length>150</length>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>City__c</fullName>
        <description>Provide a City for the Community Event Location</description>
        <externalId>false</externalId>
        <label>City</label>
