        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Public__c</fullName>
        <defaultValue>true</defaultValue>
        <description>This field drives whether the Community Event record is discoverable by other users in the Community who are not part of the group who created it.</description>
        <externalId>false</externalId>
        <inlineHelpText>If you check this box, the Event will be discoverable to all users in the Community.</inlineHelpText>
        <label>Public</label>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
