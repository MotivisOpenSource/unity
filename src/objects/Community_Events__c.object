    </fields>
    <fields>
        <fullName>Event_SF_ID__c</fullName>
        <description>Represents the Salesforce 18 Digit Unique ID for the Event record.</description>
        <externalId>false</externalId>
        <formula>CASESAFEID(Id)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Represents the Salesforce 18 Digit Unique ID for the Event record.</inlineHelpText>
        <label>Event SF ID</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Name__c</fullName>
        <description>Used to define a name for the event.  This field is mandatory.</description>
        <externalId>false</externalId>
        <label>Name</label>
