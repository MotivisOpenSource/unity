        <deleteConstraint>SetNull</deleteConstraint>
        <description>Represents the Group Terms &amp; Conditions record the User accepted.</description>
        <externalId>false</externalId>
        <inlineHelpText>Represents the Group Terms &amp; Conditions record the User accepted.</inlineHelpText>
        <label>Terms</label>
