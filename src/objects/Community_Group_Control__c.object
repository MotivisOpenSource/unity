        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Network__c</fullName>
        <description>This field defines which Community the Chatter Group will be created in.  If there is only one Community, use &quot;Community.&quot; If there are multiple Communities, use the Community Names as values for this field.</description>
        <externalId>false</externalId>
        <label>Network</label>
        <picklist>
            <picklistValues>
                <fullName>Internal</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Community</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Purpose__c</fullName>
        <description>This field defines the purpose of the group that is being created for reporting purposes.</description>
        <externalId>false</externalId>
        <inlineHelpText>What is the primary purpose for this group?</inlineHelpText>
        <label>Purpose</label>
        <picklist>
            <picklistValues>
                <fullName>Geographic</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Residence</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Course</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Team</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Club</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Project/Study Group</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <description>Represents the Status of the Group Control record.</description>
        <externalId>false</externalId>
        <inlineHelpText>Represents the Status of the Group Control record.</inlineHelpText>
        <label>Status</label>
        <picklist>
            <picklistValues>
