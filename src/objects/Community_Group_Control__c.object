    </fields>
    <fields>
        <fullName>Automatic_Archiving__c</fullName>
        <defaultValue>true</defaultValue>
        <description>This field defines whether automatic group archiving rules will apply to the related group.</description>
        <externalId>false</externalId>
        <inlineHelpText>This field defines whether automatic group archiving rules will apply to the related group.</inlineHelpText>
