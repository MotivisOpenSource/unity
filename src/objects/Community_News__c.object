    <validationRules>
        <fullName>Alert_cannot_have_Home_Page_Slot</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 Alert__c ,
 NOT(ISBLANK(TEXT( Feature_on_Home_Page_Slot__c )))
)</errorConditionFormula>
        <errorMessage>If a news item is tagged as an Alert, it should not have a value for home page slot number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Teaser_required_for_Internal</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Type__c , &apos;Internal&apos; ) &amp;&amp; ISBLANK( Teaser__c )</errorConditionFormula>
        <errorMessage>You must populate the Teaser for an Internal story</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>URL_required_for_External</fullName>
        <active>true</active>
        <description>If the News Type is External, the URL is required.</description>
        <errorConditionFormula>ISPICKVAL( Type__c , &apos;External&apos; ) &amp;&amp; ISBLANK( URL__c )</errorConditionFormula>
        <errorMessage>You must populate the URL for an External story</errorMessage>
    </validationRules>
